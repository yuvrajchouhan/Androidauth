<h1>Product</h1>

<%= form_for @product, html: { multipart: true } do |f| %>
<p>
	<%= f.label :Name %>
	<%= f.text_field :Name %>
</p>
<p>
	<%= f.label :Mobile_no  %>
	<%= f.text_field :Mobile_no %>
</p>

<p>
	<%= f.label :Address %>
	<%= f.text_field :Address %>
</p>

<div class="field">
	<%= f.label :image %><br>
	<%= f.file_field :image %>
</div>

<p>
	<%= f.submit %>
</p>

	
<% end %>